.navbar.navbar-default.navbar-fixed-top
  .container
    .navbar-header
      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand.logo(ui-sref='spells.home')
        strong Spells 서버
      - if (env === 'development')
        div.navbar-brand
          p.warn-dev TEST
    .navbar-collapse.collapse
      ul.nav.navbar-nav
        li(ui-sref-active='active', access-level='')
          a(ui-sref='spells.home') 홈
        li(ui-sref-active='active', access-level='everyone')
          a(ui-sref='spells.dashboard') 대시보드
      ul.nav.navbar-nav.navbar-right
        li(access-level='')
          a(href='/login') 로그인
        li(access-level='everyone')
          a(href='/logout') 로그아웃
  .strip
